{
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "NodeNext",
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "outDir": "dist",
    "strict": true,
    "alwaysStrict": true,
    "noImplicitThis": true,
    "strictPropertyInitialization": false,
    "useUnknownInCatchVariables": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "importHelpers": true,
    "noEmit": false,
    "allowJs": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "baseUrl": "./",
    "paths": {
      "@config": [
        "src/config"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@crons/*": [
        "src/crons/*"
      ],
      "@events/*": [
        "src/events/*"
      ],
      "@interactions/*": [
        "src/interactions/*"
      ],
      "@interface/*": [
        "src/interface/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@typings/*": [
        "src/typings/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  },
  "include": [
    "src/**/*.*",
    "environment.d.ts",
    "package.json",
    "register.cjs"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ],
  "typedocOptions": {
    "name": "Would You Documentation",
    "entryPoints": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules",
      "**/*.spec.ts"
    ],
    "externalPattern": [
      "**/node_modules/**"
    ],
    "includes": [
      "src"
    ],
    "json": "json-docs",
    "out": "docs",
    "includeVersion": true,
    "readme": "README.md",
    "showConfig": false
  }
}