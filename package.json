{
  "name": "would-you",
  "version": "3.0.0",
  "description": "Would you rather die or use this bot. You got the choice",
  "main": "dist/src/cluster.js",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint src/**/*.ts --ext .ts",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc --build --verbose",
    "copyfiles:config": "copyfiles -u 1 ./config/**/*.yaml ./dist/config",
    "copyfiles:constants": "copyfiles -u 1 ./src/constants/**/*.* ./dist/src",
    "copyfiles:env": "cp .env ./dist/.env",
    "copyfiles:register": "cp register.cjs ./dist/register.cjs",
    "copyfiles": "npm run copyfiles:config && npm run copyfiles:constants && npm run copyfiles:env && npm run copyfiles:register",
    "start": "npm run build && tsc-alias && npm run copyfiles && cd dist && node src/cluster.js",
    "dev": "nodemon --watch src/** --ext ts,json --ignore src/**/*.spec.js --exec npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Developer-Dungeon-Studio/Would-You.git"
  },
  "keywords": [
    "Discordjs",
    "discord-bot",
    "discord-js",
    "discord",
    "javascript"
  ],
  "author": "Dominik, ForGetFulSkyBro, Mezo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Developer-Dungeon-Studio/Would-You/issues"
  },
  "homepage": "https://github.com/Developer-Dungeon-Studio/Would-You#readme",
  "dependencies": {
    "@discordjs/rest": "^1.2.0",
    "@napi-rs/canvas": "^0.1.34",
    "@top-gg/sdk": "^3.1.3",
    "axios": "^1.3.4",
    "colors": "^1.4.0",
    "cron": "^2.1.0",
    "discord-api-types": "^0.37.11",
    "discord-hybrid-sharding": "^2.1.0",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "fast-glob": "^3.2.12",
    "logform": "^2.5.1",
    "moment-timezone": "^0.5.37",
    "mongoose": "^6.6.3",
    "profanity-check": "^0.0.3",
    "quickchart-js": "^3.1.2",
    "strip-ansi": "^7.0.1",
    "topgg-autoposter": "^2.0.1",
    "tsconfig-paths": "^4.2.0",
    "url": "^0.11.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "copyfiles": "^2.4.1",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "@config": "dist/src/config.js",
    "@constants/*": "dist/src/constants",
    "@crons/*": "dist/src/crons",
    "@events/*": "dist/src/events",
    "@interactions/*": "dist/src/interactions",
    "@interface/*": "dist/src/interface",
    "@models/*": "dist/src/models",
    "@services/*": "dist/src/services",
    "@typings/*": "dist/src/typings",
    "@utils/*": "dist/src/utils"
  }
}
